{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "DetectionResult",
  "type": "object",
  "properties": {
    "image_size": {
      "type": "array",
      "items": { "type": "integer" },
      "minItems": 2,
      "maxItems": 2
    },
    "boxes": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "x": { "type": "number" },
          "y": { "type": "number" },
          "w": { "type": "number" },
          "h": { "type": "number" },
          "label": { "type": "string" },
          "score": { "type": "number" }
        },
        "required": ["x","y","w","h","label","score"]
      }
    }
  },
  "required": ["image_size","boxes"]
}
